<div class="mt-4 container">
  <%= link_to "<- Back", lists_path, class: "btn btn-primary"%>
</div>

<div class="mt-4 container">
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit, class: "btn btn-primary" %>
  <% end %>
</div>
